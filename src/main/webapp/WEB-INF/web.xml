<?xml version="1.0" encoding="UTF-8"?>

<web-app     
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
    <!--http://stackoverflow.com/questions/4928271/how-to-install-jstl-the-absolute-uri-http-java-sun-com-jstl-core-cannot-be-r-->
    
    <servlet>
        <servlet-name>MultipleValuesSingleParam</servlet-name>
        <servlet-class>com.ibc.multiplevalues.MultipleValuesSingleParamServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MultipleValuesSingleParam</servlet-name>
        <url-pattern>/multiplevalues.do</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>



